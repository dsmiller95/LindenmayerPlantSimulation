#symbols S
#define |Shoot| S

r|DIFFUSION_PARAMETERS| < |Shoot|(x) : glucose < maxGlucose -> a(0, x)|Shoot|(x)

#symbols T
#matches T
r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .1 && x >  0 -> $(depth * 0.005)f(0)T(x - 1, depth)

#define |flowL1| (depth < 4)
#define L1Flower 0
#define |flowL2| (depth < 6)
#define L2Flower 0.4
#define |flowL3| (depth < 8)
#define L3Flower 0.75
#define TreeSegmentLength 5

P(1 - L1Flower) | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL1|              -> R(0)/(137)|TreeBranchCrookMarker|[-(10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(5, depth + 1)][-(-10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(TreeSegmentLength, depth + 1)]
P(L1Flower)     | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL1|              -> |Petiole|
P(1 - L2Flower) | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL2| && !|flowL1| -> R(0)/(137)|TreeBranchCrookMarker|[-(10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(5, depth + 1)][-(-10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(TreeSegmentLength, depth + 1)]
P(L2Flower)     | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL2| && !|flowL1| -> |Petiole|
P(1 - L3Flower) | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL3| && !|flowL2| -> R(0)/(137)|TreeBranchCrookMarker|[-(10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(5, depth + 1)][-(-10)i(0, 0, 0)|TreeBranchBaseMarker||DEFAULT_DIFFUSION_NODE|T(TreeSegmentLength, depth + 1)]
P(L3Flower)     | r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && |flowL3| && !|flowL2| -> |Petiole|
                  r|DIFFUSION_PARAMETERS| < T(x, depth) : glucose >= .2 && x <= 0 && !|flowL3|             -> |Petiole|


## L is a leaf. parameter is the size of the leaf, and also is is the health of the leaf which decreases when receiving a small amount of sunlight
## the leaf health decreases directly proportional the the difference between the actual sunlight and the min sunlight
#define leafCost 1
#define |Leaf| l
## l is a precursor to a leaf
#symbols lL
#matches L
r|DIFFUSION_PARAMETERS| < l(scale, finalScale, amt) : amt < leafCost && glucose >= 0.1 -> a(0, -.05)l(finalScale * amt, finalScale, amt + 0.05)
                          l(scale, finalScale, amt) : amt >= leafCost -> i(0, 0, 0)L(finalScale)

## f is a precursor to a stem
#define stemCost 1
#symbols f
#matches f
r|DIFFUSION_PARAMETERS| < f(amt) : amt < stemCost && glucose >= 0.1 -> a(0, -glucose/3)f(amt + glucose/3)
						  f(amt) : amt >= stemCost -> F(0)

## R is the resource visualizer
#symbols R
r|DIFFUSION_PARAMETERS| < R(x) -> R(counterFlow)

#define kinkDeviation 0.2

## ~ is a random kink
#symbols ~
P(1/5) | ~ ->
P(1/5) | ~ -> ^(10)
P(1/5) | ~ -> ^(-10)
P(1/5) | ~ -> +(10)
P(1/5) | ~ -> +(-10)


#symbols P
